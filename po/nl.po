msgstr "Deze optie is niet beschikbaar voor de huidige indelingsstijl"

#: panel-plugin/xkb-dialog.c:126 panel-plugin/xkb.desktop.in:4
msgid "Keyboard Layouts"
msgstr "Toetsenbordindelingen"

#: panel-plugin/xkb-dialog.c:139
msgid "Appearance"
msgstr "Uiterlijk"

#: panel-plugin/xkb-dialog.c:152
msgid "Show layout as:"
msgstr "Toon indeling als:"

#: panel-plugin/xkb-dialog.c:158
msgid "image"
msgstr "afbeelding"

#: panel-plugin/xkb-dialog.c:159
msgid "text"
msgstr "tekst"

#: panel-plugin/xkb-dialog.c:160
msgid "system"
msgstr "systeem"

#: panel-plugin/xkb-dialog.c:166
